   <div class="container">
 <p id="notice"><%= notice %></p>

<h4>Equipos</h4>

<div class="navbar "> 
<%= link_to 'Nuevo', new_equipo_path,class:'btn btn-primary btn-sm' %>
</div>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Placa</th>
      <th>Modelo</th>
      <th>Serie</th>
      <th>id Estado Equipo</th>
      <th>id Tipo Equipo</th>
      <th>Area</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equipos.each do |equipo| %>
      <tr>
        <td><%= equipo.Placa %></td>
        <td><%= equipo.Modelo %></td>
        <td><%= equipo.Serie %></td>
        <td><%= equipo.IdEstadoEquipo %></td>
        <td><%= equipo.IdTipoEquipo %></td>
        <td><%= equipo.IdArea %></td>
        <td><%= equipo.eliminado %></td>
        <td><%= link_to 'Ver', equipo %></td>
        <td><%= link_to 'Editar', edit_equipo_path(equipo) %></td>
        <td><%= link_to 'Destruir', equipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>